# Natural Language Backtesting Service - Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# ============================================
# LLM API Keys (choose one based on llm.provider in config.yaml)
# ============================================
OPENROUTER_API_KEY=your_openrouter_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# ============================================
# KIS Open API Credentials
# ============================================
# 실전투자 (Production)
KIS_APP_KEY=your_kis_app_key_here
KIS_APP_SECRET=your_kis_app_secret_here

# 모의투자 (Paper Trading)
KIS_PAPER_APP_KEY=your_kis_paper_app_key_here
KIS_PAPER_APP_SECRET=your_kis_paper_app_secret_here

# 사용자 정보
KIS_HTS_ID=your_hts_id_here

# 계좌 정보
KIS_ACCOUNT_STOCK=12345678
KIS_ACCOUNT_FUTURE=12345678
KIS_PAPER_ACCOUNT_STOCK=12345678
KIS_PAPER_ACCOUNT_FUTURE=12345678

# ============================================
# Optional Settings
# ============================================
APP_DEBUG=false
